<UserControl x:Class="MVVMFirma.Views.AllPriceHistory"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MVVMFirma.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DataGrid AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="price_history_id"/>
                <DataGridTextColumn Header="item_id"/>
                <DataGridTextColumn Header="previous_value"/>
                <DataGridTextColumn Header="new_value"/>
                <DataGridTextColumn Header="changed_by"/>
                <DataGridTextColumn Header="changed_at"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
